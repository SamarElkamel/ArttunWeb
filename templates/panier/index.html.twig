<script>
    // Get the total price from the backend or any other source
    let totalPrice = {{ totalPrice }};

    // Function to update the total price based on the selected currency
    function updateTotalPrice() {
        // Get the selected currency
        let currency = document.getElementById("currency").value;

        // Update the total price based on the selected currency
        switch (currency) {
            case "USD":
                document.getElementById("totalPrice").innerText = "$" + (totalPrice * 1.22).toFixed(2); // Assuming 1 EUR = 1.22 USD
                break;
            case "EUR":
                document.getElementById("totalPrice").innerText = totalPrice.toFixed(2) + "€";
                break;
            case "TND":
                document.getElementById("totalPrice").innerText = (totalPrice * 3.37).toFixed(2) + " TND"; // Assuming 1 EUR = 3.37 TND
                break;
            // Add cases for other currencies as needed
            default:
                // Default to EUR if the selected currency is not handled
                document.getElementById("totalPrice").innerText = totalPrice.toFixed(2) + "€";
        }
    }

    // Call the function initially to set the initial total price
    updateTotalPrice();
</script>



